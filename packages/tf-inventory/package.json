{
  "name": "@cumulus/tf-inventory",
  "version": "1.17.0",
  "description": "Package to help keep track of what resources are managed by Terraform in the AWS account",
  "main": "index.js",
  "engines": {
    "node": ">=10.16.3"
  },
  "bin": {
    "cumulus-api": "./bin/cli.js"
  },
  "scripts": {
    "test": "ava",
    "test-coverage": "nyc npm test",
    "build": "echo 'no build step'",
    "watch": "echo 'no build step'"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "fail-fast": true,
    "verbose": true
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Cumulus"
  ],
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "@cumulus/common": "1.16.1"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "nyc": "^14.0.0",
    "rewire": "^4.0.1",
    "sinon": "^8.0.2"
  }
}
